{
  "version": 2,
  "builds": [
    {
      "src": "api/[...all].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[...all].js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/[...all].js"
    }
  ],
  "functions": {
    "api/[...all].js": {
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}